{
  "name": "tune-cloud",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.4.10",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.2.1",
    "bootstrap": "5.1.0",
    "bootstrap-icons": "1.5.0",
    "react": "17.0.2",
    "react-bootstrap-icons": "1.5.0",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.1",
    "react-scripts": "^4.0.3",
